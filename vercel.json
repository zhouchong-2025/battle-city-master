{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "routes": [
    {
      "src": "/sound/(.*)",
      "dest": "/sound/$1"
    },
    {
      "src": "/resources/(.*)",
      "dest": "/resources/$1"
    },
    {
      "src": "/(.*\\.(js|css|png|jpg|gif|svg|ico|ogg))",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}